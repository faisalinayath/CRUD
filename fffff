const getData=() =>{
    axios.get('https://localhost:7041/api/cards/')
    .then((result) => {
        setData(result.data)
    }).catch((error)=>{
        console.log(error)
    })
}


const handleSave=()=>{
    const url='https://localhost:7041/api/cards/';
    const data={
        "dishName": DishName,
        "description": Description,
        "dishPrice": DishPrice
    }

    axios.post(url,data)
    .then((result)=>{
        getData();
        clear();
    })
}
