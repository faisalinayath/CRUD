
  const [data, setData] = useState([]);

  useEffect(() => {
    //setData(empData);
    getData();
  }, []);


// <------------------------------------ Used To Connect To The Web Api -----------------------------> 

const getData=() =>{
    axios.get('https://localhost:7041/api/cards/')
    .then((result) => {
        setData(result.data)
    }).catch((error)=>{
        console.log(error)
    })
}
