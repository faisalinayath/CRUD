View:- 
     <form method="post" enctype="multipart/form-data" asp-controller="Properties" asp-action="Create">

            <div class="user-box">

                <input type="text" required asp-for="Name">

                <label>Name Of The Property</label>

            </div>

            <div class="user-box">

                <input type="text" required asp-for="Price">

                <label>Price</label>

            </div>

            <div class="user-box">

                <input type="text" required asp-for="Description">

                <label>Description</label>

            </div>

            <div class="user-box">

                <input type="file" accept="image/*" asp-for="Image_Path">

            </div>

            <div class="user-box">

                <input type="text" required asp-for="Seller_Name">

                <label>Seller Name</label>

            </div>

            <div class="user-box">

                <input type="text" required asp-for="Seller_Phone">

                <label>Seller Phone Number</label>

            </div>

            <div class="user-box">

                <input type="text" required asp-for="Seller_Email_Id">

                <label>Seller Email Id</label>

            </div>

            <button type="submit">Submit</button>

            <a asp-controller="Properties" asp-action="Index">Return</a>

        </form>
        
        COntroller:-
         [HttpPost]
        [ValidateAntiForgeryToken]
        public IActionResult Create(Property obj)
        {
            _db.Property.Add(obj);
            _db.SaveChanges();
            return RedirectToAction("Index");
        }
